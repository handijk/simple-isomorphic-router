{
  "name": "simple-isomorphic-router",
  "version": "1.0.0",
  "description": "Simple isomorphic router that is easily extendable",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "format": "prettier --check \"**/*.{c,}js\"",
    "format:fix": "prettier --write \"**/*.{c,}js\"",
    "lint": "eslint {/**/,/}*.{c,}js",
    "lint:fix": "eslint --fix {/**/,/}*.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/handijk/simple-isomorphic-router.git"
  },
  "keywords": [
    "router",
    "nodejs",
    "browser",
    "javascript",
    "path-to-regexp"
  ],
  "author": "Han Dijk <info@handijk.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/handijk/simple-isomorphic-router/issues"
  },
  "homepage": "https://github.com/handijk/simple-isomorphic-router#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-no-floating-promise": "^1.0.2",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-junit": "^14.0.0",
    "prettier": "^2.7.1"
  },
  "optionalDependencies": {
    "path-to-regexp": "^6.2.1"
  }
}
